{
  "defaultSeverity": "error",
  "jsRules": {},
  "rules": {
    "member-access": false,
    "ordered-imports": false,
    "quotemark": false,
    "no-console": false,
    "semicolon": [true, "always", "ignore-bound-class-methods"],
    "jsx-no-lambda": false
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": ["config/**/*.js", "node_modules/**/*.ts"]
  }
}
